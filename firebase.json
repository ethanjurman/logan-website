{
  "hosting": {
    "public": "client/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/getAlbum",
        "function": "getAlbum"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
